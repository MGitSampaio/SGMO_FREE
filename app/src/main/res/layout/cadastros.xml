<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.Group
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgUsuario"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/configuracao"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_usuario" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/usuario"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgUsuario"
        app:layout_constraintStart_toStartOf="@+id/imgUsuario"
        app:layout_constraintTop_toBottomOf="@+id/imgUsuario" />


    <ImageView
        android:id="@+id/imgCargos"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:contentDescription="@string/cargo"
        app:layout_constraintEnd_toStartOf="@+id/imgFuncionario"
        app:layout_constraintStart_toEndOf="@+id/imgUsuario"
        app:layout_constraintTop_toTopOf="@+id/imgUsuario"
        app:srcCompat="@drawable/ic_cargo"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/cargo"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgCargos"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="@+id/imgCargos"
        app:layout_constraintTop_toBottomOf="@+id/imgCargos"
        />

    <ImageView
        android:id="@+id/imgFuncionario"
        android:layout_width="48dp"
        android:layout_height="48dp"

        android:contentDescription="@string/funcionario"
        app:layout_constraintEnd_toStartOf="@+id/imgEscalas"
        app:layout_constraintStart_toEndOf="@+id/imgCargos"
        app:layout_constraintTop_toTopOf="@+id/imgCargos"
        app:srcCompat="@drawable/ic_funcionario" />

    <ImageView
        android:id="@+id/imgCliente"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="112dp"
        android:contentDescription="@string/cliente"
        app:layout_constraintEnd_toStartOf="@+id/imgUnidade"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_cliente" />

    <ImageView
        android:id="@+id/imgUnidade"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:contentDescription="@string/unidade"
        app:layout_constraintEnd_toStartOf="@+id/imgPosto"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imgCliente"
        app:layout_constraintTop_toTopOf="@+id/imgCliente"
        app:srcCompat="@drawable/ic_unidade" />

    <TextView
        android:layout_width="83dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/funcionario"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgFuncionario"
        app:layout_constraintStart_toStartOf="@+id/imgFuncionario"
        app:layout_constraintTop_toBottomOf="@+id/imgFuncionario" />

    <TextView
        android:id="@+id/tvClientes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/cliente"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgCliente"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imgCliente"
        app:layout_constraintTop_toBottomOf="@+id/imgCliente" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/unidade"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgUnidade"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imgUnidade"
        app:layout_constraintTop_toBottomOf="@+id/imgUnidade" />

    <ImageView
        android:id="@+id/imgPosto"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:contentDescription="@string/posto"
        app:layout_constraintEnd_toStartOf="@+id/imgMaterial"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imgUnidade"
        app:layout_constraintTop_toTopOf="@+id/imgUnidade"
        app:srcCompat="@drawable/ic_posto" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/posto"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgPosto"
        app:layout_constraintStart_toStartOf="@+id/imgPosto"
        app:layout_constraintTop_toBottomOf="@+id/imgPosto" />

    <ImageView
        android:id="@+id/imgMaterial"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:contentDescription="@string/material"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imgPosto"
        app:layout_constraintTop_toTopOf="@+id/imgPosto"
        app:srcCompat="@drawable/ic_material" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/material"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgMaterial"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="@+id/imgMaterial"
        app:layout_constraintTop_toBottomOf="@+id/imgMaterial" />

    <ImageView
        android:id="@+id/imgMovimentacao"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/movimentacao"
        app:layout_constraintEnd_toEndOf="@+id/imgCliente"
        app:layout_constraintStart_toStartOf="@+id/imgCliente"
        app:layout_constraintTop_toBottomOf="@+id/tvClientes"
        app:srcCompat="@drawable/ic_movimentacao" />

    <ImageView
        android:id="@+id/imgOcorrencias"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:contentDescription="@string/ocorrencia"
        app:layout_constraintEnd_toEndOf="@+id/imgMaterial"
        app:layout_constraintStart_toStartOf="@+id/imgMaterial"
        app:layout_constraintTop_toTopOf="@+id/imgTarefas"
        app:srcCompat="@drawable/ic_ocorrencia" />

    <ImageView
        android:id="@+id/imgRonda"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:contentDescription="@string/ronda"
        app:layout_constraintEnd_toEndOf="@+id/imgUnidade"
        app:layout_constraintStart_toStartOf="@+id/imgUnidade"
        app:layout_constraintTop_toTopOf="@+id/imgMovimentacao"
        app:srcCompat="@drawable/ic_ronda" />

    <ImageView
        android:id="@+id/imgTarefas"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:contentDescription="@string/tarefa"
        app:layout_constraintEnd_toEndOf="@+id/imgPosto"
        app:layout_constraintStart_toStartOf="@+id/imgPosto"
        app:layout_constraintTop_toTopOf="@+id/imgRonda"
        app:srcCompat="@drawable/ic_vencimento" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/ronda"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgRonda"
        app:layout_constraintHorizontal_bias="0.272"
        app:layout_constraintStart_toStartOf="@+id/imgRonda"
        app:layout_constraintTop_toBottomOf="@+id/imgRonda" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/ocorrencia"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgOcorrencias"
        app:layout_constraintStart_toStartOf="@+id/imgOcorrencias"
        app:layout_constraintTop_toBottomOf="@+id/imgOcorrencias" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/tarefa"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgTarefas"
        app:layout_constraintStart_toStartOf="@+id/imgTarefas"
        app:layout_constraintTop_toBottomOf="@+id/imgTarefas" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/movimentacao"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgMovimentacao"
        app:layout_constraintStart_toStartOf="@+id/imgMovimentacao"
        app:layout_constraintTop_toBottomOf="@+id/imgMovimentacao" />

    <ImageView
        android:id="@+id/imgEscalas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgFuncionario"
        app:layout_constraintTop_toTopOf="@+id/imgFuncionario"
        app:srcCompat="@drawable/ic_escala"
        android:contentDescription="TODO" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/escala"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgEscalas"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imgEscalas"
        app:layout_constraintTop_toBottomOf="@+id/imgEscalas" />

    <androidx.constraintlayout.widget.Group
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imgUniforme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/imgRonda"
        app:layout_constraintStart_toStartOf="@+id/imgMovimentacao"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/ic_uniforme" />

    <ImageView
        android:id="@+id/imgVencimento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/imgOcorrencias"
        app:layout_constraintStart_toStartOf="@+id/imgTarefas"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/ic_vencimento" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Uniformes"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgUniforme"
        app:layout_constraintStart_toStartOf="@+id/imgUniforme"
        app:layout_constraintTop_toBottomOf="@+id/imgUniforme" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Vencimentos"
        android:textColor="@color/colorPrimary"
        app:layout_constraintEnd_toEndOf="@+id/imgVencimento"
        app:layout_constraintStart_toStartOf="@+id/imgVencimento"
        app:layout_constraintTop_toBottomOf="@+id/imgVencimento" />

    <com.google.android.gms.ads.AdView
        android:id="@+id/bannerCadastros"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        app:adSize="BANNER"
        app:adUnitId="ca-app-pub-5510724908004504/6155812609"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_editor_absoluteX="0dp"></com.google.android.gms.ads.AdView>

</androidx.constraintlayout.widget.ConstraintLayout>